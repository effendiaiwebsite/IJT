{"examId":"ibps-clerk","subjectId":"quantitative-aptitude","chapterId":"time-work","totalQuestions":6,"totalMarks":6,"duration":"6 minutes","questions":[{"id":"q1","questionNumber":1,"questionText":"A completes work in 10 days. 1 day work?","options":["1/5","1/10","1/15","1/20"],"correctAnswer":1,"explanation":"1/10 of work per day","difficulty":"easy","marks":1},{"id":"q2","questionNumber":2,"questionText":"A does in 10 days, B in 15 days. Together?","options":["5 days","6 days","8 days","12 days"],"correctAnswer":1,"explanation":"1/10+1/15=1/6, so 6 days","difficulty":"medium","marks":1},{"id":"q3","questionNumber":3,"questionText":"12 men finish work in 8 days. 6 men take?","options":["12 days","16 days","20 days","24 days"],"correctAnswer":1,"explanation":"Inverse: 12×8 = 6×x, x=16","difficulty":"medium","marks":1},{"id":"q4","questionNumber":4,"questionText":"A and B together in 12 days, A alone in 20 days. B alone?","options":["25 days","30 days","35 days","40 days"],"correctAnswer":1,"explanation":"1/B=1/12-1/20=1/30","difficulty":"hard","marks":1},{"id":"q5","questionNumber":5,"questionText":"If 5 workers = 10 days, 10 workers = ?","options":["5 days","10 days","15 days","20 days"],"correctAnswer":0,"explanation":"Inverse proportion: 5 days","difficulty":"easy","marks":1},{"id":"q6","questionNumber":6,"questionText":"A's 1 day work = 1/20, B's = 1/30. Together?","options":["10 days","12 days","15 days","18 days"],"correctAnswer":1,"explanation":"1/20+1/30=5/60=1/12","difficulty":"medium","marks":1}]}
